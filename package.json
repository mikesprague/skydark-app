{
  "name": "skydark-app",
  "version": "3.3.0",
  "description": "Open source PWA built as an homage to the now defunct Dark Sky for Android app",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">= 18.x",
    "npm": ">= 8.x"
  },
  "scripts": {
    "build": "vite build",
    "clean": "rm -rf ./build",
    "dev": "vite serve",
    "prebuild": "npm run clean",
    "preview": "vite preview",
    "start": "cross-env NODE_ENV=development wrangler pages dev -- vite serve",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "repository": {
    "url": "https://github.com/mikesprague/skydark-app"
  },
  "author": {
    "name": "Michael Sprague",
    "email": "mikesprague@gmail.com"
  },
  "homepage": "https://skydark.app",
  "license": "MIT",
  "dependencies": {
    "@bugsnag/core": "7.19.0",
    "@bugsnag/js": "7.20.2",
    "@bugsnag/plugin-react": "7.19.0",
    "@fortawesome/fontawesome-svg-core": "6.4.0",
    "@fortawesome/free-brands-svg-icons": "6.4.0",
    "@fortawesome/free-regular-svg-icons": "6.4.0",
    "@fortawesome/free-solid-svg-icons": "6.4.0",
    "@fortawesome/pro-duotone-svg-icons": "6.4.0",
    "@fortawesome/pro-light-svg-icons": "6.4.0",
    "@fortawesome/pro-regular-svg-icons": "6.4.0",
    "@fortawesome/pro-solid-svg-icons": "6.4.0",
    "@fortawesome/pro-thin-svg-icons": "6.4.0",
    "@fortawesome/react-fontawesome": "0.2.0",
    "@sweetalert2/theme-dark": "5.0.15",
    "@tippyjs/react": "4.2.6",
    "axios": "1.4.0",
    "beautiful-react-hooks": "4.3.0",
    "cross-env": "7.0.3",
    "dayjs": "1.11.9",
    "dotenv": "16.3.1",
    "jose": "4.14.4",
    "leaflet": "1.9.4",
    "nanoid": "4.0.2",
    "prop-types": "15.8.1",
    "query-string": "8.1.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-google-charts": "4.0.0",
    "react-leaflet": "4.2.1",
    "register-service-worker": "1.7.2",
    "sweetalert2": "11.7.12",
    "sweetalert2-react-content": "5.0.7",
    "tippy.js": "6.3.7",
    "use-local-storage-state": "18.3.3"
  },
  "devDependencies": {
    "@babel/eslint-parser": "7.22.6",
    "@babel/eslint-plugin": "7.22.5",
    "@babel/preset-env": "7.22.6",
    "@fullhuman/postcss-purgecss": "5.0.0",
    "@tailwindcss/forms": "0.5.3",
    "@tailwindcss/typography": "0.5.9",
    "@vitejs/plugin-react-swc": "3.3.2",
    "autoprefixer": "10.4.14",
    "babel-plugin-add-react-displayname": "0.0.5",
    "cssnano": "6.0.1",
    "cz-git": "1.6.1",
    "eslint": "8.44.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-prettier": "8.8.0",
    "eslint-config-stylelint": "19.0.0",
    "eslint-plugin-compat": "4.1.4",
    "eslint-plugin-html": "7.1.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-markdown": "3.0.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "postcss": "8.4.24",
    "postcss-cli": "10.1.0",
    "prettier": "2.8.8",
    "prettier-eslint": "15.0.1",
    "sass": "1.63.6",
    "stylelint": "15.9.0",
    "stylelint-config-standard": "33.0.0",
    "stylelint-config-standard-scss": "10.0.0",
    "stylelint-prettier": "3.0.0",
    "tailwindcss": "3.3.2",
    "vite": "4.3.9",
    "vite-plugin-html": "3.2.0",
    "vite-plugin-pwa": "0.16.4",
    "wrangler": "3.1.1"
  },
  "babel": {
    "plugins": [
      "add-react-displayname"
    ],
    "presets": [
      "@babel/preset-env"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "commonjs": true,
      "es6": true,
      "node": true
    },
    "settings": {
      "jest": {
        "version": 28
      }
    },
    "extends": [
      "airbnb",
      "eslint:recommended",
      "plugin:react/recommended",
      "plugin:react-hooks/recommended",
      "plugin:jsx-a11y/recommended",
      "prettier",
      "stylelint"
    ],
    "plugins": [
      "compat",
      "html",
      "import",
      "jsx-a11y",
      "markdown",
      "prettier",
      "react",
      "react-hooks"
    ],
    "parserOptions": {
      "ecmaVersion": "latest",
      "allowImportExportEverywhere": true,
      "parser": "@eslint/babel-parser",
      "ecmaFeatures": {
        "jsx": true,
        "impliedStrict": true,
        "modules": true
      },
      "sourceType": "module"
    },
    "rules": {
      "n/no-missing-import": [
        "warn"
      ],
      "react/jsx-filename-extension": [
        "error",
        {
          "extensions": [
            ".js",
            ".jsx"
          ]
        }
      ],
      "react/function-component-definition": [
        2,
        {
          "namedComponents": "arrow-function",
          "unnamedComponents": "arrow-function"
        }
      ],
      "react/destructuring-assignment": "warn",
      "react-hooks/rules-of-hooks": "error",
      "react-hooks/exhaustive-deps": "error",
      "react/jsx-boolean-value": "off",
      "jest/no-deprecated-functions": "off",
      "jsx-a11y/no-noninteractive-element-interactions": "off",
      "jsx-a11y/no-static-element-interactions": "off",
      "jsx-a11y/click-events-have-key-events": "off",
      "jsx-a11y/no-onchange": "off",
      "consistent-return": "off",
      "no-console": "warn"
    }
  },
  "prettier": {
    "semi": true,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "all"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-git"
    }
  }
}
