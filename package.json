{
  "name": "skydark-app",
  "version": "0.1.0",
  "description": "Open source PWA built as an homage to the now defunct Dark Sky for Android app",
  "private": true,
  "engines": {
    "node": ">= 12.x",
    "npm": ">= 6.x",
    "yarn": ">= 1.22.x"
  },
  "scripts": {
    "build": "yarn clean && yarn webpack",
    "clean": "rm -rf ./build",
    "prepublish": "yarn snyk",
    "snyk": "snyk protect",
    "start": "cross-env NODE_ENV=development vercel dev",
    "test": "echo \"Error: no test specified\" && exit 1",
    "webpack": "webpack --color"
  },
  "keywords": [],
  "repository": {
    "url": "https://github.com/mikesprague/skydark-app"
  },
  "author": {
    "name": "Michael Sprague",
    "email": "mikesprague@gmail.com"
  },
  "homepage": "https://skydark.app",
  "license": "MIT",
  "dependencies": {
    "@bugsnag/core": "^7.11.0",
    "@bugsnag/js": "^7.11.0",
    "@bugsnag/plugin-react": "^7.11.0",
    "@tippyjs/react": "^4.2.5",
    "axios": "^0.22.0",
    "cogo-toast": "^4.2.3",
    "dayjs": "^1.10.7",
    "history": "^5.0.1",
    "leaflet": "^1.7.1",
    "logrocket": "^2.0.0",
    "logrocket-react": "^4.0.1",
    "nanoid": "^3.1.28",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-google-charts": "^3.0.15",
    "react-leaflet": "^3.2.1",
    "react-router-dom": "6.0.0-beta.3",
    "register-service-worker": "^1.7.2",
    "snyk": "^1.730.0"
  },
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@babel/eslint-parser": "^7.15.7",
    "@babel/eslint-plugin": "^7.14.5",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-proposal-export-default-from": "^7.14.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.15.6",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-object-assign": "^7.14.5",
    "@babel/plugin-transform-runtime": "^7.15.0",
    "@babel/preset-env": "^7.15.6",
    "@babel/preset-react": "^7.14.5",
    "@babel/runtime": "^7.15.4",
    "@fortawesome/fontawesome-svg-core": "^1.3.0-beta2",
    "@fortawesome/free-brands-svg-icons": "^6.0.0-beta2",
    "@fortawesome/free-regular-svg-icons": "^6.0.0-beta2",
    "@fortawesome/free-solid-svg-icons": "^6.0.0-beta2",
    "@fortawesome/pro-duotone-svg-icons": "^6.0.0-beta2",
    "@fortawesome/pro-light-svg-icons": "^6.0.0-beta2",
    "@fortawesome/pro-regular-svg-icons": "^6.0.0-beta2",
    "@fortawesome/pro-solid-svg-icons": "^6.0.0-beta2",
    "@fortawesome/pro-thin-svg-icons": "^6.0.0-beta2",
    "@fortawesome/react-fontawesome": "^0.1.15",
    "@fullhuman/postcss-purgecss": "^4.0.3",
    "@octokit/core": "^3.5.1",
    "@rollup/plugin-node-resolve": "^13.0.5",
    "@tailwindcss/forms": "^0.4.0-alpha.1",
    "@tailwindcss/typography": "^0.5.0-alpha.2",
    "@typescript-eslint/eslint-plugin": "^4.32.0",
    "@typescript-eslint/parser": "^4.32.0",
    "autoprefixer": "^10.3.6",
    "babel-loader": "^8.2.2",
    "babel-plugin-add-react-displayname": "^0.0.5",
    "compression-webpack-plugin": "^9.0.0",
    "copy-webpack-plugin": "^9.0.1",
    "cross-env": "^7.0.3",
    "css-loader": "^6.3.0",
    "css-minimizer-webpack-plugin": "^3.0.2",
    "cssnano": "^5.0.8",
    "dotenv": "^10.0.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-compat": "^3.13.0",
    "eslint-plugin-flowtype": "^6.1.0",
    "eslint-plugin-html": "^6.2.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-markdown": "^2.2.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-security": "^1.4.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.3.2",
    "mini-css-extract-plugin": "^2.3.0",
    "node-sass": "^6.0.1",
    "postcss": "^8.3.8",
    "postcss-cli": "^9.0.1",
    "postcss-loader": "^6.1.1",
    "prettier": "^2.4.1",
    "prettier-eslint": "^13.0.0",
    "prettier-stylelint": "^0.4.2",
    "rollup": "^2.58.0",
    "sass-lint": "^1.13.1",
    "sass-loader": "^12.1.0",
    "style-loader": "^3.3.0",
    "tailwindcss": "^3.0.0-alpha.1",
    "terser-webpack-plugin": "^5.2.4",
    "typescript": "^4.4.3",
    "url-loader": "^4.1.1",
    "vercel": "^23.1.2",
    "webpack": "^5.56.0",
    "webpack-cli": "^4.8.0",
    "workbox-webpack-plugin": "^6.3.0"
  },
  "babel": {
    "plugins": [
      "@babel/plugin-proposal-class-properties",
      "@babel/plugin-proposal-export-default-from",
      "@babel/plugin-proposal-object-rest-spread",
      "@babel/plugin-syntax-dynamic-import",
      "@babel/plugin-transform-object-assign",
      "@babel/plugin-transform-runtime",
      "add-react-displayname"
    ],
    "presets": [
      "@babel/preset-env",
      "@babel/react"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "es6": true,
      "node": true
    },
    "extends": [
      "airbnb",
      "eslint:recommended",
      "plugin:react/recommended",
      "plugin:react-hooks/recommended",
      "plugin:jsx-a11y/recommended",
      "prettier"
    ],
    "plugins": [
      "compat",
      "html",
      "import",
      "jsx-a11y",
      "markdown",
      "prettier",
      "react",
      "react-hooks",
      "security"
    ],
    "parserOptions": {
      "ecmaVersion": 11,
      "allowImportExportEverywhere": true,
      "parser": "babel-parser",
      "ecmaFeatures": {
        "jsx": true
      },
      "sourceType": "module"
    },
    "rules": {
      "react/jsx-filename-extension": [
        "error",
        {
          "extensions": [
            ".js",
            ".jsx"
          ]
        }
      ],
      "react-hooks/rules-of-hooks": "error",
      "react-hooks/exhaustive-deps": "error",
      "react/jsx-boolean-value": "off",
      "jsx-a11y/no-noninteractive-element-interactions": "off",
      "jsx-a11y/no-static-element-interactions": "off",
      "jsx-a11y/click-events-have-key-events": "off",
      "jsx-a11y/no-onchange": "off",
      "consistent-return": "off"
    }
  },
  "prettier": {
    "printWidth": 120,
    "semi": true,
    "singleQuote": true,
    "tabwidth": true,
    "trailingComma": "all"
  }
}
