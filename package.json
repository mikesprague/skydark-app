    "history": "5.3.0",
    "leaflet": "1.7.1",
    "logrocket": "2.2.1",
    "logrocket-react": "5.0.1",
    "nanoid": "3.3.2",
    "prop-types": "15.8.1",
    "react": "18.0.0",
