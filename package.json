{
  "name": "skydark-app",
  "version": "0.1.0",
  "description": "Open source PWA built as an homage to the now defunct Dark Sky for Android app",
  "private": true,
  "engines": {
    "node": ">= 16.x",
    "npm": ">= 8.x"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production npm run webpack",
    "clean": "rm -rf ./build",
    "dev": "concurrently 'npm run watch' 'npm run serve' --names 'ðŸ“¦,ðŸ–¥ï¸' --prefix '[{name}] ' --kill-others",
    "prebuild": "npm run clean",
    "serve": "serve -S ./build -p 3000",
    "start": "cross-env NODE_ENV=development npm run dev",
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch": "cross-env NODE_ENV=development npm run webpack -- watch --mode development",
    "webpack": "webpack --color"
  },
  "keywords": [],
  "repository": {
    "url": "https://github.com/mikesprague/skydark-app"
  },
  "author": {
    "name": "Michael Sprague",
    "email": "mikesprague@gmail.com"
  },
  "homepage": "https://skydark.app",
  "license": "MIT",
  "dependencies": {
    "@bugsnag/core": "7.17.0",
    "@bugsnag/js": "7.17.0",
    "@bugsnag/plugin-react": "7.17.0",
    "@fortawesome/fontawesome-svg-core": "6.1.1",
    "@fortawesome/free-brands-svg-icons": "6.1.1",
    "@fortawesome/free-regular-svg-icons": "6.1.1",
    "@fortawesome/free-solid-svg-icons": "6.1.1",
    "@fortawesome/pro-duotone-svg-icons": "6.1.1",
    "@fortawesome/pro-light-svg-icons": "6.1.1",
    "@fortawesome/pro-regular-svg-icons": "6.1.1",
    "@fortawesome/pro-solid-svg-icons": "6.1.1",
    "@fortawesome/pro-thin-svg-icons": "6.1.1",
    "@fortawesome/react-fontawesome": "0.2.0",
    "@tailwindcss/forms": "0.5.2",
    "@tailwindcss/typography": "0.5.4",
    "@tippyjs/react": "4.2.6",
    "axios": "0.27.2",
    "dayjs": "1.11.4",
    "dotenv": "16.0.1",
    "history": "5.3.0",
    "leaflet": "1.8.0",
    "logrocket": "3.0.1",
    "logrocket-react": "5.0.1",
    "nanoid": "4.0.0",
    "prop-types": "15.8.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-google-charts": "4.0.0",
    "react-leaflet": "4.0.1",
    "react-router-dom": "6.3.0",
    "register-service-worker": "1.7.2",
    "sweetalert2": "11.4.24",
    "tailwindcss": "3.1.6"
  },
  "devDependencies": {
    "@babel/core": "7.18.9",
    "@babel/eslint-parser": "7.18.9",
    "@babel/eslint-plugin": "7.17.7",
    "@babel/plugin-proposal-class-properties": "7.18.6",
    "@babel/plugin-proposal-export-default-from": "7.18.9",
    "@babel/plugin-proposal-object-rest-spread": "7.18.9",
    "@babel/plugin-syntax-dynamic-import": "7.8.3",
    "@babel/plugin-transform-object-assign": "7.18.6",
    "@babel/plugin-transform-react-jsx": "7.18.6",
    "@babel/plugin-transform-runtime": "7.18.9",
    "@babel/preset-env": "7.18.9",
    "@babel/preset-react": "7.18.6",
    "@babel/runtime": "7.18.9",
    "@fullhuman/postcss-purgecss": "4.1.3",
    "autoprefixer": "10.4.7",
    "babel-loader": "8.2.5",
    "babel-plugin-add-react-displayname": "0.0.5",
    "compression-webpack-plugin": "10.0.0",
    "concurrently": "7.3.0",
    "copy-webpack-plugin": "11.0.0",
    "cross-env": "7.0.3",
    "css-loader": "6.7.1",
    "css-minimizer-webpack-plugin": "4.0.0",
    "cssnano": "5.1.12",
    "eslint": "8.20.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-prettier": "8.5.0",
    "eslint-config-stylelint": "15.1.0",
    "eslint-plugin-compat": "4.0.2",
    "eslint-plugin-html": "7.0.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsx-a11y": "6.6.1",
    "eslint-plugin-markdown": "3.0.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.30.1",
    "eslint-plugin-react-hooks": "4.6.0",
    "file-loader": "6.2.0",
    "html-webpack-plugin": "5.5.0",
    "mini-css-extract-plugin": "2.6.1",
    "postcss": "8.4.14",
    "postcss-cli": "10.0.0",
    "postcss-loader": "7.0.1",
    "prettier": "2.7.1",
    "prettier-eslint": "15.0.1",
    "sass": "1.54.0",
    "sass-loader": "13.0.2",
    "serve": "14.0.1",
    "style-loader": "3.3.1",
    "stylelint": "14.9.1",
    "stylelint-config-prettier": "9.0.3",
    "stylelint-config-prettier-scss": "0.0.1",
    "stylelint-config-standard": "26.0.0",
    "stylelint-config-standard-scss": "5.0.0",
    "stylelint-prettier": "2.0.0",
    "terser-webpack-plugin": "5.3.3",
    "url-loader": "4.1.1",
    "vercel": "27.2.0",
    "webpack": "5.74.0",
    "webpack-cli": "4.10.0",
    "workbox-webpack-plugin": "6.5.3"
  },
  "babel": {
    "plugins": [
      "@babel/plugin-proposal-class-properties",
      "@babel/plugin-proposal-export-default-from",
      "@babel/plugin-proposal-object-rest-spread",
      "@babel/plugin-syntax-dynamic-import",
      "@babel/plugin-transform-object-assign",
      "@babel/plugin-transform-runtime",
      "add-react-displayname"
    ],
    "presets": [
      "@babel/preset-env",
      "@babel/react"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "commonjs": true,
      "es6": true,
      "node": true
    },
    "extends": [
      "airbnb",
      "eslint:recommended",
      "plugin:react/recommended",
      "plugin:react-hooks/recommended",
      "plugin:jsx-a11y/recommended",
      "prettier",
      "stylelint"
    ],
    "plugins": [
      "compat",
      "html",
      "import",
      "jsx-a11y",
      "markdown",
      "prettier",
      "react",
      "react-hooks"
    ],
    "parserOptions": {
      "parser": "babel-parser",
      "ecmaVersion": 11,
      "allowImportExportEverywhere": true,
      "sourceType": "module",
      "ecmaFeatures": {
        "impliedStrict": true,
        "modules": true,
        "jsx": true
      }
    },
    "rules": {
      "react/jsx-filename-extension": [
        "error",
        {
          "extensions": [
            ".js",
            ".jsx"
          ]
        }
      ],
      "react/function-component-definition": [
        2,
        {
          "namedComponents": "arrow-function",
          "unnamedComponents": "arrow-function"
        }
      ],
      "react-hooks/rules-of-hooks": "error",
      "react-hooks/exhaustive-deps": "error",
      "react/jsx-boolean-value": "off",
      "jest/no-deprecated-functions": "off",
      "jsx-a11y/no-noninteractive-element-interactions": "off",
      "jsx-a11y/no-static-element-interactions": "off",
      "jsx-a11y/click-events-have-key-events": "off",
      "jsx-a11y/no-onchange": "off",
      "consistent-return": "off"
    }
  },
  "prettier": {
    "semi": true,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "all"
  }
}
